{
    "name": "streaming-frontend-canvas",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://stream.peerchecker.com",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "start-remote": "vite --mode staging --config vite.config.remote.ts",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "deploy": "yarn build && NODE_DEBUG=gh-pages gh-pages -d dist"
    },
    "dependencies": {
        "peerbit": "^4",
        "@peerbit/document": "^6",
        "@peerbit/react": "*",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-use": "^17.4.0",
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@mui/material": "^5.15.7",
        "react-router-dom": "^6.16.0",
        "@mui/icons-material": "^5.15.7"
    },
    "devDependencies": {
        "@types/audioworklet": "^0.0.46",
        "@types/dom-mediacapture-transform": "^0.1.4",
        "@webgpu/types": "^0.1.32",
        "@types/dom-webcodecs": "^0.1.11",
        "@types/react": "^18.2.46",
        "@types/react-dom": "^18.2.18",
        "@vitejs/plugin-react": "^4",
        "typescript": "^5.3.3",
        "vite": "^5.0.12"
    },
    "browserslist": {
        "production": [
            "chrome >= 67",
            "edge >= 79",
            "firefox >= 68",
            "opera >= 54",
            "safari >= 14"
