    "version": "0.1.1",
    "private": true,
    "homepage": "https://stream.peerchecker.com",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "start-remote": "vite --mode staging --config vite.config.remote.ts",
