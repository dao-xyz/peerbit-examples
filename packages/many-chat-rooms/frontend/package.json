        "@peerbit/example-many-chat-rooms": "^0.0.1",
        "@peerbit/react": "*",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-use": "^17.4.0",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/material": "^5.10.13",
        "react-router-dom": "^6.8.0",
        "@mui/icons-material": "^5.10.16"
    },
    "devDependencies": {
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@vitejs/plugin-react": "^4.0.3",
        "typescript": "^5.1.3",
        "vite": "^4.4.4",
        "gh-pages": "^4.0.0"
    },
    "browserslist": {
        "production": [
            "chrome >= 67",
            "edge >= 79",
