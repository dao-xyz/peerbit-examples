{
    "name": "chat-room",
    "version": "0.0.1",
    "private": true,
    "homepage": "https://dao-xyz.github.io/peerbit-examples",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "start-remote": "vite --mode staging --config vite.config.remote.ts",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "deploy": "yarn build && NODE_DEBUG=gh-pages gh-pages -d dist"
    },
    "dependencies": {
        "peerbit": "^4",
        "@peerbit/document": "^9",
        "@peerbit/peer-names": "^0.0.1",
        "@peerbit/react": "*",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-use": "^17.6.0",
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@mui/material": "^5.15.7",
        "react-router": "^7.9.3",
        "@mui/icons-material": "^7.0.2"
    },
    "devDependencies": {
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^5",
        "typescript": "^5.6.3",
        "vite": "^7.1.3",
        "@peerbit/vite": "^1"
    },
    "browserslist": {
        "production": [
            "chrome >= 67",
            "edge >= 79",
            "firefox >= 68",
            "opera >= 54",
            "safari >= 14"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
