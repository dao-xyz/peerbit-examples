{
    "name": "chat-room",
    "version": "0.0.1",
    "private": true,
    "homepage": "https://dao-xyz.github.io/peerbit-examples",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "start-remote": "vite --mode staging --config vite.config.remote.ts",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "deploy": "yarn build && NODE_DEBUG=gh-pages gh-pages -d dist"
    },
    "dependencies": {
        "peerbit": "^4",
        "@peerbit/document": "^6",
        "@peerbit/peer-names": "^0.0.1",
        "@peerbit/react": "*",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-use": "^17.4.0",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/material": "^5.10.13",
        "react-router-dom": "^6.16.0",
        "@mui/icons-material": "^5.10.16"
    },
    "devDependencies": {
        "@types/react": "^18.2.46",
        "@types/react-dom": "^18.2.18",
        "@vitejs/plugin-react": "^4",
        "typescript": "^5.3.3",
        "vite": "^5.0.12"
    },
    "browserslist": {
        "production": [
            "chrome >= 67",
            "edge >= 79",
            "firefox >= 68",
            "opera >= 54",
            "safari >= 14"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
