{
    "name": "@giga-app/interface",
    "version": "0.0.23",
    "author": "dao.xyz",
    "repository": "https://github.com/@dao-xyz/peerbit-examples",
    "license": "Apache-2.0",
    "type": "module",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "module": "lib/esm/index.js",
    "types": "lib/esm/index.d.ts",
    "exports": {
        "import": "./lib/esm/index.js"
    },
    "files": [
        "lib",
        "src",
        "!src/**/__tests__",
        "!lib/**/__tests__",
        "LICENSE"
    ],
    "engines": {
        "node": ">=16.15.1"
    },
    "scripts": {
        "clean": "shx rm -rf lib/*",
        "build": "yarn clean && tsc -p tsconfig.json",
        "test": "vitest run",
        "test:watch": "vitest",
        "test-replicator": "npm run build && node ./lib/esm/__tests__/replicator-node.js",
        "test-replicator-dial": "DIAL_LOCAL=true npm run build && node ./lib/esm/__tests__/replicator-node.js",
        "test-replicator-remote": "npm run build && node ./lib/esm/__tests__/replicator-node-remote.js"
    },
    "devDependencies": {
        "@peerbit/test-utils": "^2",
        "@types/qrcode": "^1.5.5",
        "typescript": "^5.6.3",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@peerbit/document": "^9",
        "peerbit": "^4",
        "qrcode": "^1.5.4",
        "uuid": "^10.0.0"
    }
}
