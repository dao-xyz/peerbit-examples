{
    "name": "@giga-app/interface-frontend",
    "version": "2.0.0",
    "private": true,
    "homepage": "https://dao-xyz.github.io/peerbit-examples",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:debug": "VITE_DEBUG=true vite",
        "start": "vite",
        "start-https": "vite",
        "start-remote": "vite --mode staging --config vite.config.remote.ts",
        "prefetch:update": "tsx --tsconfig tsconfig.scripts.json scripts/update-prefetch.ts",
        "build": "tsc && npm run prefetch:update && vite build",
        "build:test": "tsc --project tsconfig.test.json && vite build",
        "preview": "vite preview",
        "deploy": "pnpm run build && NODE_DEBUG=gh-pages gh-pages -d dist",
        "pretest:e2e": "pnpm --filter @giga-app/interface run build",
        "test:e2e": "playwright test",
        "pretest:e2e:headed": "pnpm --filter @giga-app/interface run build",
        "test:e2e:headed": "playwright test --headed",
        "pretest:e2e:ui": "pnpm --filter @giga-app/interface run build",
        "test:e2e:ui": "playwright test --ui",
        "test:unit": "vitest run"
    },
    "dependencies": {
        "@giga-app/app-service": "workspace:*",
        "@giga-app/interface": "workspace:*",
        "@giga-app/llm": "workspace:*",
        "@giga-app/sdk": "workspace:*",
        "@headlessui/react": "^2.2.0",
        "@peerbit/document-react": "^0",
        "@peerbit/peer-names": "workspace:*",
        "@peerbit/program-react": "^0.2.0",
        "@peerbit/react": "^0",
        "@radix-ui/react-radio-group": "^1.3.7",
        "@radix-ui/react-toggle": "^1.0.3",
        "@radix-ui/react-toggle-group": "^1.0.3",
        "axios": "^1.4.0",
        "framer-motion": "^12.23.3",
        "lodash.debounce": "^4.0.8",
        "lodash.throttle": "^4.1.1",
        "mdast-util-from-markdown": "^2.0.2",
        "mdast-util-to-string": "^4.0.0",
        "peerbit": "^4",
        "react": "^19.2.0",
        "react-color": "^2.19.3",
        "react-dom": "^19.1.0",
        "react-grid-layout-next": "^2.2.0",
        "react-icons": "^5.5.0",
        "react-markdown": "^10.1.0",
        "react-router": "^7.9.3",
        "react-use": "^17.6.0",
        "remark-gfm": "^4.0.1",
        "uint8arrays": "^5.1.0"
    },
    "devDependencies": {
        "@peerbit/vite": "^1.1.0",
        "@playwright/test": "^1.57.0",
        "@tailwindcss/vite": "^4.0.14",
        "@types/lodash": "^4.17.16",
        "@types/lodash.debounce": "^4.0.9",
        "@types/lodash.throttle": "^4.1.9",
        "@types/react": "^19.1.8",
        "@types/react-color": "^3.0.13",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-basic-ssl": "^2.1.0",
        "@vitejs/plugin-react": "^5",
        "autoprefixer": "^10.4.20",
        "tailwindcss": "^4.0.14",
        "tsx": "^4.19.2",
        "typescript": "^5.6.3",
        "vite": "^7.1.3"
    },
    "browserslist": {
        "production": [
            "chrome >= 67",
            "edge >= 79",
            "firefox >= 68",
            "opera >= 54",
            "safari >= 14"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
